al] = useState();\n\n    useEffect(() => {\n        fetch('http://localhost:5000/cart/get_total/' + id)\n            .then(response => response.json())\n            .then(total => {\n                setTotal(total);\n            })\n    }, [change])\n\n\n    return (\n        <div className={classes.cart}>\n            \n            {items.map(item => (\n                <BillItem item={item} addToCart={handleAddToCart} removeFromCart={handleRemoveFromCart}/>\n            ))}\n\n            <div className={classes.total}>\n                <span className={classes.totalText}>Tổng cộng:</span>\n                <span className={classes.totalPrice}>{total || 0}VNĐ</span>\n            </div>\n        </div>\n\n    )\n}\n\nexport default Bill;","C:\\Users\\Asus\\Downloads\\Compressed\\nmCNPM-main\\nmCNPM-main\\lirvecafe\\src\\frontend\\components\\UserStore\\index.js",[],"C:\\Users\\Asus\\Downloads\\Compressed\\nmCNPM-main\\nmCNPM-main\\lirvecafe\\src\\frontend\\components\\products\\product\\product.jsx",["511","512","513"],"import React from 'react';\nimport {Card, CardMedia, CardContent, CardActions, Typography, IconButton} from '@material-ui/core';\nimport {Button} from '@material-ui/core';\nimport {AddShoppingCart} from '@material-ui/icons';\nimport useStyles from './style';\nimport * as ROUTES from '../../constants/routes/routes';\nimport * as isSignined from '../../constants/isSignined';\nimport {getCookie, deleteCookie} from \"../../constants/userCookie\";\n\nconst Product = ({product}) => {\n    const classes = useStyles();\n\n    function titlechange(string) {\n        if (string.length > 18) {\n            string = string.substring(0, 15) + \"...\";\n        }\n        return string;\n    }\n\n    let signined = getCookie(isSignined.customer);\n    let user = getCookie('customer');\n    if (user) {\n        // console.log(typeof user);\n        // console.log(user);\n        user = JSON.parse(user);\n        //console.log(user);\n    } else {\n        user = {}\