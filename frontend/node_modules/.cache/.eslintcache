 \"./UpdateStaff.module.css\"\nfunction DeleteStaff(props) {\n    // console.log(props);\n    const staffInfo = props.staff;\n    // console.log(staffInfo);\n    \n    // const currentInfo = {\n    //     id: staffInfo.id,\n    //     name: name,\n    //     phone: phone,\n    //     address: address,\n    //     email: email\n    // }\n\n    const handleDeleteStaffInfo = () => {\n        //Hàm Xóa-API tương ứng ID lấy từ staffInfo\n        axios({\n            method: 'DELETE',\n            url: 'http://localhost:5000/staff/delete',\n            data: {\n                id: staffInfo.id,\n            }\n        })\n        .then(res => {\n            // console.log(res.status);\n            if(res.status === 200) {\n                props.closeModal(false);\n                props.setFlagSuccess(true);\n            }\n            \n        }) \n        .catch(error => {\n            console.log(error);\n        })\n        //Xóa thất bại\n        //Xóa thành công\n        // alert(\"xóa thành công!\");\n        \n    }\n    return (\n        <div className={styles.DeleteStaff}>\n            <div className={styles.form} >\n                <span onClick={() => props.closeModal(false)} className={styles.form_exit}>&times;</span>\n                <h1 className={styles.form_heading}>Xác nhận xóa tài khoản</h1>\n                \n                <div className={styles.field}>\n                    <label className={styles.form_label}>ID: {staffInfo.id}</label>\n                </div>\n                <div className={styles.field}>\n                    <label className={styles.form_label}>Tên nhân viên: {staffInfo.name}</label>\n                </div>\n               \n                \n                <div className={styles.field_submit}>\n                    <button onClick={() => handleDeleteStaffInfo()} className={styles.form_submit} type=\"submit\">Xác nhận</button>\n                    <button onClick={() => props.closeModal(false)} className={styles.form_submit_cancel} type=\"submit\">Hủy xóa</button>\n                </div>\n            </div>\n        </div>\n    );\n}\nexport default DeleteStaff;","C:\\Users\\Asus\\Downloads\\Compressed\\nmCNPM-main\\nmCNPM-main\\lirvecafe\\src\\frontend\\components\\HomeStaff\\Orders\\OrdersList.js",["522","523"],"import React from 'react'\nimport { Table, TableBody, TableCell, TableContainer, TableHead, TableRow, Paper } from '@material-ui/core';\nimport styles from './Orders.module.css'\nconst OrdersList = ({ orders, remove, update }) => {\n    // var state = orders[0].state;\n    // console.log(orders);\n    return (\n        <TableContainer component={Paper}>\n            <Table aria-label=\"a dense table\">\n                <TableHead>\n                    <TableRow>\n                        <TableCell >Tên khách hàng</TableCell>\n                        <TableCell align=\"right\">Số điện thoại</TableCell>\n                        <TableCell align=\"right\">Tổng số tiền</TableCell>\n                        <TableCell align=\"right\">Địa chỉ</TableCell>\n                        <TableCell align=\"right\">Trạng thái</TableCell>\n                    </TableRow>\n                </TableHead>\n                <TableBody>\n                    {orders.map((order) => (\n                        <TableRow\n                            id={order.billId}\n                            key={order.billId}\n                            sx={{ '&:last-child td, &:last-child th': { border: 0 } }}\n                        >\n                            <TableCell component=\"th\" scope=\"row\">\n                                {order.name}\n                            </TableCell>\n                            <TableCell align=\"right\">{order.phone}</TableCell>\n                            <TableCell align=\"right\">{order.total}</TableCell>\n                            <TableCell align=\"right\">{order.shippingAddress}</TableCell>\n                            <TableCell align=\"right\">\n                                {\n                                    (order.state == 0) ? (\n                                        <>\n                                            <button className={styles.btn_confirm} onClick={() => update(order)}>Xác nhận</button>\n                                            <button className={styles.btn_delete} onClick={() => remove(order)}>Hủy</button>\n                                        </>\n                                    ) : (order.state == 1) ? (\n                                        <>\n                                            <button className={styles.btn_complete} onClick={() => update(order)}>Hoàn thành</button>\n                                        </>\n                                    ) : (\n                                        <>\n                                        </>\n                                    )\n                                }\n                            </TableCell>\n                        </TableRow>\n                    ))}\n                </TableBody>\n            </Table>\n        </TableContainer>\n\n    )\n}\nexport default OrdersList\n","C:\\Users\\Asus\\Downloads\\Compressed\\nmCNPM-main\\nmCNPM-main\\lirvecafe\\src\\frontend\\components\\HomeAdmin\\CRUD\\SuccessFrom.js",["524","525"],"import { Link } from \"react-router-dom\";\nimport { useState } from \"react\";\nimport styles from \"./CRUD.module.css\"\n// import styles from \"./UpdateStaff.module.css\"\nfunction SuccessForm(props) {\n    // console.log(props);\n    \n    // const submitSucessForm = () => {\n    //     alert(\"xóa thành công!\")\n    // }\n    return (\n        <div className={styles.SuccessForm}>\n            <div className={styles.form} >\n                <span onClick={() => props.closeModal(false)} className={styles.form_exit}>&times;</span>\n                <h1 className={styles.form_heading}>{props.success} Nhấn OK để quay trở lại</h1>\n                \n                {/* <div className={styles.field}>\n                    <label className={styles.form_label}>ID: {staffInfo.id}</label>\n                </div>\n                <div className={styles.field}>\n                    <label className={styles.form_label}>Tên nhân viên: {staffInfo.name}</label>\n                </div> */}\n               \n                \n                <div className={styles.field_submit}>\n                    <button onClick={() => {props.setFlagSuccess(false); props.setChange(!props.isUpdated)}} className={styles.form_submit} type=\"submit\">OK</button>\n                </div>\n            </div>\n        </div>\n    );\n}\nexport default SuccessForm;","C:\\Users\\Asus\\Downloads\\Compressed\\nmCNPM-main\\nmCNPM-main\\lirvecafe\\src\\frontend\\components\\HomeStaff\\Events\\UpdateEvent.js",[],"C:\\Users\\Asus\\Downloads\\Compressed\\nmCNPM-main\\nmCNPM-main\\lirvecafe\\src\\frontend\\components\\HomeStaff\\Products\\ProductList.js",["526","527"],"import React from 'react'\nimport { useState } from 'react';\nimport { Table, TableBody, TableCell, TableContainer, TableHead, TableRow, Paper } from '@material-ui/core';\nimport UpdateProduct from './UpdateProduct';\nimport styles from './Products.module.css'\nconst ProductList = ({ change, IsChange, type, products, remove }) => {\n    const [isOpen, setIsOpen] = useState(false);\n    const [product, setProduct] = useState([]);\n    function handleUpdateProduct(product) {\n        setIsOpen(true);\n        setProduct(product);\n    }\n    // console.log(product);\n    return (\n        <>\n            {isOpen && <UpdateProduct change={change} IsChange={IsChange} setIsOpen={setIsOpen} product={product}></UpdateProduct>}\n            <TableContainer component={Paper}>\n                <Table aria-label=\"a dense table\">\n                    <TableHead>\n                        <TableRow>\n                            <TableCell align=\"center\">Hình ảnh</TableCell>\n                            <TableCell align=\"center\">Tên sản phẩm</TableCell>\n                            <TableCell align=\"center\">Phân loại</TableCell>\n                            <TableCell align=\"center\">Giá tiền</TableCell>\n                            <TableCell align=\"center\">Số lượng còn lại</TableCell>\n                            <TableCell align=\"center\">Hành động</TableCell>\n                        </TableRow>\n                    </TableHead>\n                    <TableBody>\n                        {products.map((product) => (\n                            (product.type == type) ? (\n                                <TableRow\n                                    id={product.productId}\n                                    key={product.productId}\n                                    sx={{ '&:last-child td, &:last-child th': { border: 0 } }}\n                                    style={{ 'padding': '4px 8px' }}\n                                >\n                                    <TableCell component=\"th\" scope=\"row\" align=\"center\">\n                                        <img src={product.image} className={styles.img}></img>\n                                    </TableCell>\n                                    <TableCell align=\"center\">{product.name}</TableCell>\n                                    <TableCell align=\"center\">{product.type}</TableCell>\n                                    <TableCell align=\"center\">{product.price}</TableCell>\n                                    <TableCell align=\"center\">{product.stock}</TableCell>\n                                    <TableCell align=\"center\">\n                                        <button className={styles.btn_update} onClick={() => handleUpdateProduct(product)}>Sửa</button>\n                                        <button className={styles.btn_delete} onClick={() => remove(product)}>Xóa</button>\n                                    </TableCell>\n                                </TableRow>\n                            ) : (\n                                <></>\n                            )\n                        ))}\n                    </TableBody>\n                </Table>\n            </TableContainer>\n        </>\n    )\n}\nexport default ProductList\n","C:\\Users\\Asus\\Downloads\\Compressed\\nmCNPM-main\\nmCNPM-main\\lirvecafe\\src\\frontend\\components\\HomeAdmin\\CRUD\\AddStaff.js",["528"],"import { set, useForm } from \"react-hook-form\";\nimport { useRef, useState } from \"react\";\nimport { yupResolver } from \"@hookform/resolvers/yup\";\nimport * as yup from \"yup\";\nimport styles from \"./CRUD.module.css\"\nimport { Link } from \"react-router-dom\";\nimport axios from \"axios\";\nconst schema = yup.object().shape({\n    name: yup\n        .string()\n        .required(\"Vui lòng nhập tên nhân viên!\")\n        .max(50, \"Tên tối đa 50 ký tự!\"),\n    phone: yup\n        .string()\n        .required(\"Vui lòng nhập số điện thoại!\"),\n    address: yup\n        .string()\n        .required(\"Vui lòng nhập địa chỉ!\"),\n    email: yup\n        .string()\n        .required(\"Vui lòng nhập email!\"),\n    password: yup\n        .string()\n        .required(\"Vui lòng nhập mật khẩu!\")\n        .min(7, \"Mật khẩu phải có nhiều hơn 6 ký tự\")\n});\nexport default function AddStaff (props) {\n    // console.log(props);\n    const {\n        register,\n        reset,\n        setFocus,\n        handleSubmit,\n        formState: {errors}\n    } = useForm({resolver: yupResolver(schema)});\n    const inputRef = useRef();\n    const [msg, setMsg] = useState('');\n    const [err, setErr] = useState('');\n    const onSubmit = (data) => {\n        // console.log(data);\n        setMsg('');\n        setErr('');\n        axios({\n            method: 'POST',\n            url: 'http://localhost:5000/staff/add',\n            data: {\n                phone: data.phone,\n                password: data.password, \n                name: data.name,\n                email: data.email,\n                address: data.a